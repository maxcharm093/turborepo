{
  "name": "my-turborepo",
  "version": "1.0.1",
  "private": true,
  "keywords": [
    "turbo",
    "monorepo",
    "pnpm",
    "typescript",
    "react",
    "nextjs",
    "nestjs",
    "typeorm",
    "swc",
    "tailwindcss",
    "eslint",
    "prettier",
    "husky",
    "lint-staged",
    "commitizen",
    "cz-conventional-changelog",
    "changeset",
    "jest",
    "typeorm-seeding",
    "typeorm-fixtures",
    "typeorm-transactional-tests",
    "typeorm-transactional-cls-hooked",
    "typeorm-transactional-cls-hooked-fixtures",
    "typeorm-transactional-cls-hooked-seeding",
    "typeorm-transactional-cls-hooked-fixtures-seeding"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aungphyoex/turbo-npn.git"
  },
  "license": "MIT",
  "author": {
    "name": "Aung Pyae Phyo",
    "email": "hi@aungpyaephyo.com",
    "url": "https://aungpyaephyo.com",
    "github": "aungphyoex"
  },
  "scripts": {
    "add:api": "pnpm add --filter=api",
    "add:web": "pnpm add --filter=web",
    "build": "turbo build",
    "changeset": "changeset",
    "clear:modules": "npx npkill",
    "commit": "cz",
    "dev": "turbo dev",
    "dev:api": "pnpm dev --filter=api",
    "dev:web": "pnpm dev --filter=web",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint": "turbo lint",
    "prepare": "husky",
    "prod": "turbo prod",
    "test": "turbo test"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.10",
    "@commitlint/config-conventional": "^19.6.0",
    "@types/node": "^22.0.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "prettier": "^3.3.3",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.6",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "turbo": "^2.3.3",
    "typescript": "5.5.4",
    "zod": "^3.24.1"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=20"
  }
}
